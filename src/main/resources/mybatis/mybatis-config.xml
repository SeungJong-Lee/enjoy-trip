    <typeAlias type="com.ssafy.enjoytrip.trail.dto.TrailInputDto" alias="trailInputDto"/>
    <typeAlias type="com.ssafy.enjoytrip.trail.dto.TrailBoardDto" alias="trailBoardDto"/>
    <typeAlias type="com.ssafy.enjoytrip.trail.dto.CityDto" alias="cityDto"/> 
    <typeAlias type="com.ssafy.enjoytrip.trail.dto.TrailBoardReplyDto" alias="trailBoardReplyDto"/> 
  </typeAliases>

  <mappers>
    <mapper resource="mybatis/mapper/userMapper.xml"/>
    <mapper resource="mybatis/mapper/noticeMapper.xml"/>
    <mapper resource="mybatis/mapper/AttractionMapper.xml"/>
    <mapper resource="mybatis/mapper/PlaceMapper.xml"/>
    <mapper resource="mybatis/mapper/planMapper.xml"/>
    <mapper resource="mybatis/mapper/trailMapper.xml"/>
  </mappers>
</configuration>