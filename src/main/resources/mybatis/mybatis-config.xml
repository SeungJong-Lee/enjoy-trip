<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
    <typeAlias type="com.ssafy.enjoytrip.user.dto.UserDto" alias="userDto"/>
    <typeAlias type="com.ssafy.enjoytrip.user.dto.JoinDto" alias="joinDto"/>
    <typeAlias type="com.ssafy.enjoytrip.user.dto.FollowDto" alias="followDto"/>
    <typeAlias type="com.ssafy.enjoytrip.notice.dto.NoticeDto" alias="noticeDto"/>
    <typeAlias type="com.ssafy.enjoytrip.Attraction.dto.AttractionDto" alias="attractionDto"/>
    <typeAlias type="com.ssafy.enjoytrip.Attraction.dto.SidoCodeDto" alias="sidoCodeDto"/>
    <typeAlias type="com.ssafy.enjoytrip.Attraction.dto.GugunCodeDto" alias="gugunCodeDto"/>
    <typeAlias type="com.ssafy.enjoytrip.place.dto.PlaceDto" alias="placeDto"/>
    <typeAlias type="com.ssafy.enjoytrip.place.dto.ReplyDto" alias="replyDto"/>
    <typeAlias type="com.ssafy.enjoytrip.place.dto.RecommendDto" alias="recommendDto"/>
    <typeAlias type="com.ssafy.enjoytrip.plan.dto.PlanWriteRequestDto" alias="planWriteRequestDto"/>
